{
  "name": "gemini-schedule-demo",
  "version": "1.0.0",
  "description": "A demo application showing how to use Google's Gemini API to create student schedules",
  "main": "dist/taskbank-tests.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/taskbank-tests.js",
    "dev": "ts-node --transpile-only taskbank-tests.ts",
    "test": "npm run build && node dist/taskbank-tests.js",
    "test1": "npm run build && node -e \"require('./dist/taskbank-tests.js').testObviousSuggestionsFewTasks()\"",
    "test2": "npm run build && node -e \"require('./dist/taskbank-tests.js').testObviousSuggestionsSomeTasks()\"",
    "test3": "npm run build && node -e \"require('./dist/taskbank-tests.js').testObviousSuggestionsManyTasks()\"",
    "test4": "npm run build && node -e \"require('./dist/taskbank-tests.js').testUnrelatedTasks()\"",
    "test5": "npm run build && node -e \"require('./dist/taskbank-tests.js').testTasksWithManyDependencies()\"",
    "test6": "npm run build && node -e \"require('./dist/taskbank-tests.js').testNoDescriptions()\""

  },
  "keywords": ["gemini", "ai", "schedule", "student", "demo", "typescript"],
  "author": "Student Demo",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  }
}
